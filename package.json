{
  "name": "jira-precommit-hook",
  "version": "2.0.1",
  "description": "Git commit hook to verify commit messages are tagged with a JIRA issue number",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint ./",
    "prepublish": "npm run test && npm run build",
    "test": "mocha && npm run lint",
    "install": "node bin/install"
  },
  "keywords": [
    "jira",
    "git",
    "precommit-hook"
  ],
  "author": "Matthew L Smith <mtscout6@gmail.com>",
  "contributors": [
    "Curtis Knox",
    "Karl Thompson",
    "Matthew Radcliffe",
    "Steven Tran",
    "Devin Wall",
    "Trevor Martz",
    "Matt Smith"
  ],
  "files": [
    "bin",
    "hooks",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-config-exchange-solutions": "1.0.0",
    "mocha": "^2.2.4",
    "rimraf": "^2.3.4",
    "sinon": "^1.14.1",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.7.0"
  },
  "dependencies": {
    "babel-runtime": "^6.3.13",
    "colors": "^1.1.0",
    "es6-promise": "^3.0.2",
    "fs-promise": "^0.3.1",
    "jira": "^0.9.2",
    "lodash": "^4.0.0",
    "request-promise": "^2.0.0"
  },
  "repository": {
