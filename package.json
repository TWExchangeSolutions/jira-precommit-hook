{
  "name": "jira-precommit-hook",
  "version": "1.1.3",
  "description": "Git commit hook to verify commit messages are tagged with a JIRA issue number",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint ./",
    "prepublish": "npm run test && npm run build",
    "test": "mocha && npm run lint",
    "install": "node bin/install"
  },
  "keywords": [
    "jira",
    "git",
    "precommit-hook"
  ],
  "author": "Matthew L Smith <mtscout6@gmail.com>",
  "contributors": [
    "Curtis Knox",
    "Karl Thompson",
    "Matthew Radcliffe",
    "Steven Tran"
  ],
  "files": [
    "bin",
    "hooks",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-eslint": "^4.0.5",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.0.0",
    "eslint": "^0.24.1",
    "mocha": "^2.2.4",
    "rimraf": "^2.3.4",
    "sinon": "^1.14.1",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.7.0"
  },
  "dependencies": {
    "babel-runtime": "^5.4.7",
    "colors": "^1.1.0",
    "es6-promise": "^2.1.1",
    "fs-promise": "^0.3.1",
    "jira": "^0.9.2",
    "lodash": "^3.8.0",
    "request-promise": "^0.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TWExchangeSolutions/jira-precommit-hook.git"
  },
  "bugs": {
    "url": "https://github.com/TWExchangeSolutions/jira-precommit-hook/issues"
  },
  "homepage": "https://github.com/TWExchangeSolutions/jira-precommit-hook#readme"
}
